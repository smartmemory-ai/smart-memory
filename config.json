{
  "graph_db": {
    "backend_class": "FalkorDBBackend",
    "host": "${FALKORDB_HOST:-localhost}",
    "port": "${FALKORDB_PORT:-9010}",
    "graph_name": "${GRAPH_NAME:-ontology}"
  },
  "semantic": {},
  "episodic": {},
  "procedural": {},
  "working": {},
  "zettelkasten": {},
  "working_memory": {
    "persist": "${WORKING_MEMORY_PERSIST:-true}",
    "vectorize": "${WORKING_MEMORY_VECTORIZE:-false}",
    "retention_days": "${WORKING_MEMORY_RETENTION_DAYS:-14}",
    "bypass_pipeline": "${WORKING_MEMORY_BYPASS_PIPELINE:-true}"
  },
  "vector": {
    "default": "${VECTOR_DEFAULT:-falkordb}",
    "type": "${VECTOR_TYPE:-falkordb}",
    "dimension": "${VECTOR_DIMENSION:-1536}",
    "metric": "${VECTOR_METRIC:-cosine}",
    "hnsw_m": "${VECTOR_HNSW_M:-16}",
    "hnsw_ef_construction": "${VECTOR_HNSW_EF_CONSTRUCTION:-200}",
    "hnsw_ef_runtime": "${VECTOR_HNSW_EF_RUNTIME:-64}",
    "host": "${FALKORDB_HOST:-localhost}",
    "port": "${FALKORDB_PORT:-9010}",
    "embedding": {
      "provider": "${EMBEDDING_PROVIDER:-openai}",
      "model": "${EMBEDDING_MODEL:-text-embedding-ada-002}",
      "openai_api_key": "${OPENAI_API_KEY:-}"
    }
  },
  "ontology": {
    "enabled": "${ONTOLOGY_ENABLED:-false}",
    "inference": {
      "engine": "${ONTOLOGY_INFERENCE_ENGINE:-basic}",
      "ontogpt": {
        "enabled": "${ONTOLOGY_ONTOGPT_ENABLED:-false}",
        "provider": "${ONTOGPT_PROVIDER:-openai}",
        "model": "${ONTOGPT_MODEL:-gpt-4o-mini}",
        "api_base": "${ONTOGPT_API_BASE:-}",
        "api_version": "${ONTOGPT_API_VERSION:-}",
        "template": "${ONTOGPT_TEMPLATE:-}",
        "template_dir": "${ONTOGPT_TEMPLATE_DIR:-}",
        "confidence_threshold": "${ONTOLOGY_CONFIDENCE_THRESHOLD:-0.6}"
      }
    }
  },
  "extractor": {
    "default": "${EXTRACTOR_DEFAULT:-llm}",
    "relik": {
      "model_name": "${RELIK_MODEL:-relik-ie/relik-relation-extraction-small-wikipedia-ner}"
    },
    "gliner": {
      "model_name": "${GLINER_MODEL:-urchade/gliner_base}"
    },
    "llm": {
      "model_name": "${EXTRACTOR_LLM_MODEL:-gpt-5-mini}",
      "openai_api_key": "${OPENAI_API_KEY:-}"
    },
    "spacy": {
      "model_name": "${SPACY_MODEL:-en_core_web_sm}"
    },
    "rebel": {
      "model_name": "${REBEL_MODEL:-Babelscape/rebel-large}"
    },
    "gpt4o_triple": {
      "model_name": "gpt-4.1-mini",
      "openai_api_key": "${OPENAI_API_KEY:-}"
    }
  },
  "enricher": {
    "default": "default",
    "temporal": {
      "model_name": "${ENRICHER_TEMPORAL_MODEL:-gpt-4o-mini}",
      "openai_api_key": "${OPENAI_API_KEY:-}"
    }
  },
  "cache": {
    "host": "${REDIS_HOST:-localhost}",
    "redis": {
      "host": "${REDIS_HOST:-localhost}",
      "port": "${REDIS_PORT:-9012}",
      "db": "${REDIS_DB:-0}",
      "password": "${REDIS_PASSWORD:-null}",
      "socket_timeout": "${REDIS_SOCKET_TIMEOUT:-5}",
      "socket_connect_timeout": "${REDIS_SOCKET_CONNECT_TIMEOUT:-5}",
      "retry_on_timeout": "${REDIS_RETRY_ON_TIMEOUT:-true}",
      "health_check_interval": "${REDIS_HEALTH_CHECK_INTERVAL:-30}"
    },
    "ttl": {
      "embedding_ttl": "${EMBEDDING_TTL:-86400}",
      "search_ttl": "${SEARCH_TTL:-900}",
      "entity_extraction_ttl": "${ENTITY_EXTRACTION_TTL:-3600}",
      "similarity_ttl": "${SIMILARITY_TTL:-1800}"
    }
  },
  "mongodb": {
    "host": "${MONGODB_HOST:-localhost}",
    "port": "${MONGODB_PORT:-9013}",
    "database": "${MONGODB_DATABASE:-smart_memory}",
    "username": "${MONGODB_USERNAME:-}",
    "password": "${MONGODB_PASSWORD:-}",
    "auth_source": "${MONGODB_AUTH_SOURCE:-admin}",
    "connection_timeout": "${MONGODB_CONNECTION_TIMEOUT:-10000}",
    "server_selection_timeout": "${MONGODB_SERVER_SELECTION_TIMEOUT:-5000}"
  },
  "background": {
    "mode": "redis"
  },
  "adapter": {
    "default": "${ADAPTER_DEFAULT:-default}"
  },
  "converter": {
    "default": "${CONVERTER_DEFAULT:-default}"
  },
  "observability": {
    "enabled": true,
    "unified_keys": true
  },
  "auth": {
    "provider": "${AUTH_PROVIDER:-dummy}",
    "dummy": {
      "mock_users_enabled": "${AUTH_DUMMY_MOCK_USERS:-true}",
      "default_admin": "${AUTH_DUMMY_DEFAULT_ADMIN:-user_1}"
    },
    "jwt": {
      "secret_key": "${AUTH_JWT_SECRET:-}",
      "algorithm": "${AUTH_JWT_ALGORITHM:-HS256}",
      "access_token_expire_minutes": "${AUTH_JWT_EXPIRE_MINUTES:-30}",
      "issuer": "${AUTH_JWT_ISSUER:-smart-memory}"
    },
    "oauth": {
      "google": {
        "client_id": "${AUTH_GOOGLE_CLIENT_ID:-}",
        "client_secret": "${AUTH_GOOGLE_CLIENT_SECRET:-}"
      }
    },
    "database": {
      "user_table": "${AUTH_DB_USER_TABLE:-users}",
      "connection_string": "${AUTH_DB_CONNECTION:-}"
    }
  },
  "analytics": {
    "sentiment": {
      "enabled": "${ANALYTICS_SENTIMENT_ENABLED:-true}",
      "backend": "${ANALYTICS_SENTIMENT_BACKEND:-vader}"
    },
    "topic": {
      "enabled": "${ANALYTICS_TOPIC_ENABLED:-true}",
      "model": "${ANALYTICS_TOPIC_MODEL:-lda}",
      "n_topics": "${ANALYTICS_TOPIC_K:-15}",
      "min_df": "${ANALYTICS_MIN_DF:-2}",
      "max_features": "${ANALYTICS_MAX_FEATURES:-20000}",
      "min_docs": "${ANALYTICS_MIN_DOCS:-10}"
    },
    "bias": {
      "js_alert_threshold": "${ANALYTICS_JS_THRESHOLD:-0.3}",
      "protected_attributes_default": "${ANALYTICS_PROTECTED_ATTRS:-gender,race,age_group,nationality,religion}"
    }
  }
}
